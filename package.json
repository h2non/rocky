{
  "name": "rocky",
  "version": "0.4.9",
  "description": "Full-featured, middleware-oriented, hackable HTTP and WebSocket proxy",
  "repository": "h2non/rocky",
  "author": "Tomas Aparicio",
  "license": "MIT",
  "engines": {
    "node": ">= 0.12"
  },
  "scripts": {
    "test": "standard 'lib/**/*.js' 'test/**/*.js' && mocha --timeout 10000 --bail --reporter spec --ui tdd test/*.js test/*/*.js test/*/*/*.js",
    "bench": "bash benchmark/run.sh"
  },
  "keywords": [
    "http",
    "websocket",
    "proxy",
    "http-proxy",
    "reverse",
    "migration",
    "migrate",
    "repeat",
    "replay",
    "forward",
    "traffic",
    "gateway",
    "router",
    "middleware",
    "intercept",
    "interceptor",
    "capture",
    "sniffer",
    "mitm",
    "balancer",
    "load",
    "hackable"
  ],
  "dependencies": {
    "http-proxy": "^1.12.0",
    "indexport": "^0.1.1",
    "is-fail": "^0.1.1",
    "lodash": "^3.9.3",
    "media-typer": "^0.3.0",
    "midware-pool": "^0.1.1",
    "raw-body": "^2.1.2",
    "retry": "^0.6.1",
    "router": "^1.1.1"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "connect": "^3.4.0",
    "express": "^4.13.0",
    "fw": "^0.1.2",
    "http-version": "^0.1.0",
    "mocha": "^2.2.5",
    "request": "^2.64.0",
    "rocky-consul": "^0.1.0",
    "sinon": "^1.15.3",
    "standard": "^5.4.1",
    "supertest": "^1.0.1",
    "vhost": "^3.0.0",
    "ws": "^0.8.0"
  },
  "standard": {
    "globals": [
      "suite",
      "test",
      "beforeEach",
      "afterEach"
    ]
  }
}
